{
	"manifest_version": 2,

	"name": "__MSG_app_name__",
	"description": "__MSG_app_description__",
	"version": "1.0.1",
	"default_locale": "en",

	"permissions": [
		"*://*.ebay.es/*", "*://*.ebay.fr/*", "*://*.ebay.it/*"
	],

	"icons": {
		"128": "icon128.png"
	},

	"content_scripts": [
		{
			"matches": ["*://*.ebay.es/*", "*://*.ebay.fr/*", "*://*.ebay.it/*"],
			"js": ["untranslate.js"],
			"run_at": "document_end"
    		}
	]
}
